<!-- new body parts - artificial -->

    <RecipeDef ParentName="SurgeryFlesh">
        <defName>InstallBionicEyeAnimal</defName>
        <label>install bionic animal eye</label>
        <description>Installs bionic animal eye. Requires a min skill in medicine of 8.</description>
        <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
        <jobString>Installing bionic animal eye.</jobString>
        <workAmount>2000</workAmount>
		<skillRequirements>
            <li>
                <skill>Medicine</skill>
                <minLevel>8</minLevel>
            </li>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>1</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>BionicEyeAnimal</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
            <thingDefs>
                <li>BionicEyeAnimal</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>RightEye</li>
            <li>LeftEye</li>
        </appliedOnFixedBodyParts>
        <addsHediff>BionicEye</addsHediff>
    </RecipeDef>

